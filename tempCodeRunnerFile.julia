print("U\tA\tB\tA∪B\tA∩B\tA'\tB'")
for i in range(len(U)):
    print(f"{U[i]}\t{A[i]:.1f}\t{B[i]:.1f}\t{Union[i]:.1f}\t{Intersection[i]:.1f}\t{Complement_A[i]:.1f}\t{Complement_B[i]:.1f}")
